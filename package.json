{
  "name": "tra-api",
  "version": "0.0.1",
  "description": "A guide to REST API using Typescript created for Toptal blog",
  "main": "index.js",
  "scripts": {
    "watch": "ts-node-dev --respawn app.ts",
    "deploy": "tsc --build --clean && tsc -p . && sls deploy",
    "start": "ts-node --transpile-only app.ts",
    "dev": "tsc && node dist/app.js",
    "build": "tsc -p .",
    "clean": "tsc --build --clean",
    "start:prod": "node dist/server.js",
    "debug": "export DEBUG=* && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makinhs/toptal-rest-series.git"
  },
  "keywords": [
    "REST",
    "API",
    "ExpressJS",
    "NodeJS",
    "Toptal"
  ],
  "author": "Marcos Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makinhs/toptal-rest-series/issues"
  },
  "homepage": "https://github.com/makinhs/toptal-rest-series#readme",
  "dependencies": {
    "argon2": "^0.40.3",
    "aws-lambda": "^1.0.7",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "mongoose": "^8.4.4",
    "multer": "^1.4.5-lts.1",
    "serverless-http": "^3.2.0",
    "serverless-offline": "^13.6.0",
    "shortid": "^2.2.16",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/shortid": "0.0.32",
    "source-map-support": "^0.5.16",
    "tslint": "^6.0.0",
    "typescript": "^5.1.3"
  }
}
